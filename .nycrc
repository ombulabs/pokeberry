{
  "extension": [".js", ".ts", ".tsx"],
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "all": true,
  "excludeAfterRemap": true,
  "include": ["packages/**/*.tsx"],
  "exclude": ["packages/**/index.ts", "packages/**/**/index.ts", "packages/**/*.stories.tsx"],
  "cache": false,
  "report-dir": "coverage/coverage-cypress"
}
